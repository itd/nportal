<html>
  <body>
    <div metal:use-macro="view.layout">
      <div metal:fill-slot="content" class="main-content">
        <div class="container-fluid">

          <div class="row-fluid">
            <h2 tal:content="page_title | nothing"></h2>
            <p tal:content="page_description | nothing"></p>
          </div>

          <!--!<div tal:content="structure form"></div>!-->
          <div class="row-fluid">
            <div class="col-sm-12">
              <h1><span tal:replace="data.givenName"/>
                <span tal:replace="data.middleName"/> <span tal:replace="data.sn"/>
                <span tal:replace="data.suffix"/></h1>
            </div>
            <div style="clear: both; height: 1px;"></div>
          </div>

          <div class="row-fluid">
            <div class="col-sm-12">
              submitted: <span tal:replace="python:data.subTimestamp.strftime('%Y-%m-%d %-I:%M %p')"/>
            </div>
            <div style="clear: both; height: 1px;"></div>
          </div>


          <form id="req_edit" method="POST" class="deform" accept-charset="utf-8"
            tal:attributes="action action">
          <div class="row-fluid form-box">

            <div class="col-sm-4">
              <div class="form-horizontal">

                <div class="form-group">
                  <label for="givenName"
                  class="col-sm-3 control-label">First</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="givenName"
                    tal:attributes="value data.givenName" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="middleName"
                  class="col-sm-3 control-label">Middle</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="middleName"
                    tal:attributes="value data.middleName" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="sn"
                         class="col-sm-3 control-label">Last</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="sn"
                    tal:attributes="value data.sn" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="suffix"
                         class="col-sm-3 control-label">Suffix</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="suffix"
                    tal:attributes="value data.suffix" readonly>
                  </div>
                </div>

              </div>
            </div>

            <div class="col-sm-4">
              <div class="form-horizontal">

                <div class="form-group">
                  <label for="mail"
                  class="col-sm-3 control-label">Email</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="mail"
                    tal:attributes="value data.mail" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="phone"
                  class="col-sm-3 control-label">Phone</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="phone"
                    tal:attributes="value data.phone" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="cell" class="col-sm-3 control-label">Cell</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="cell"
                    tal:attributes="value data.cell" readonly>
                  </div>
                </div>

              </div>
            </div>

            <div class="col-sm-4">
              <div class="form-horizontal">



                <div class="form-group">
                  <label for="street"
                      class="col-sm-3 control-label">Street Addr</label>
                  <div class="col-sm-9">
                      <input type="text" class="form-control"
                             id="street" name="street"
                             tal:attributes="value data.street" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="lcity"
                      class="col-sm-3 control-label">City</label>
                  <div class="col-sm-9">
                      <input type="text" class="form-control"
                             id="lcity" name="lcity"
                             tal:attributes="value data.lcity" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="st"
                      class="col-sm-3 control-label">State/Prov</label>
                  <div class="col-sm-9">
                      <input type="text" class="form-control"
                             id="st" name="st"
                             tal:attributes="value data.st" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="postalCode"
                      class="col-sm-3 control-label">Post/Zip Code</label>
                  <div class="col-sm-9">
                      <input type="text" class="form-control"
                             id="postalCode" name="postalCode"
                             tal:attributes="value data.postalCode" readonly>
                  </div>
                </div>
                <div class="form-group">
                  <label for="country"
                      class="col-sm-3 control-label">Country</label>
                  <div class="col-sm-9">
                      <input type="text" class="form-control"
                             id="country" name="country"
                             tal:attributes="value data.country" readonly>
                  </div>
                </div>

              </div>
            </div>

            <div style="clear: both; height: 1px;"></div>
          </div> <!--!end row -->



          <div class="row-fluid form-box">
            <div class="col-sm-4">
              <div class="form-horizontal">

                <div class="form-group">
                  <label for="birthCountry"
                      class="col-sm-3 control-label">Country of Birth</label>
                  <div class="col-sm-9">
                      <input type="text" class="form-control"
                             id="birthCountry" name="birthCountry"
                             tal:attributes="value data.birthCountry" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="phone"
                  class="col-sm-3 control-label">Citizenships</label>
                  <div class="col-sm-9">
                    <tal:rep tal:repeat="c data.citizenships">
                      <input tal:attributes="value c.code"
                        type="radio" name="citizenships" checked/> ${c.name}
                    </tal:rep>
                  </div>
                </div>



              </div>
            </div>
            <div style="clear: both; height: 1px;"></div>
          </div> <!-- end row 2 -->



          <div class="row-fluid form-box">
            <div class="col-sm-4">
              <div class="form-horizontal">

                <div class="form-group">
                  <label for="employerType"
                  class="col-sm-3 control-label">employerType</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="employerType"
                    tal:attributes="value data.employerType" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for="employerName"
                  class="col-sm-3 control-label">employerName</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="employerName"
                    tal:attributes="value data.employerName" readonly>
                  </div>
                </div>

              </div>
            </div>
            <div style="clear: both; height: 1px;"></div>
          </div> <!-- end row 3 -->


          <div class="row-fluid form-box">


            <div class="col-sm-4">
              <div class="form-horizontal">

                <div class="form-group">
                  <label for="nrelUserID"
                  class="col-sm-3 control-label">nrelUserID</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="nrelUserID"
                    tal:attributes="value data.nrelUserID" readonly>
                  </div>
                </div>

                <div class="form-group">
                  <label for=" preferredUID"
                  class="col-sm-3 control-label"> preferredUID</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id=" preferredUID"
                    tal:attributes="value data. preferredUID" readonly>
                  </div>
                </div>

              </div>
            </div>



            <div class="col-sm-8">
              <div class="form-horizontal">

                <div class="form-group">
                  <label for="justification"
                  class="col-sm-2 control-label">Justification</label>
                  <div class="col-sm-10">
                    <div class="just-comm" tal:content="data.justification"/>
                  </div>
                </div>

                <div class="form-group">
                  <label for="comments"
                  class="col-sm-2 control-label">Comments</label>
                  <div class="col-sm-10">
                    <div class="just-comm" tal:content="data.comments"/>
                  </div>
                </div>

              </div>
            </div>
            <div style="clear: both; height: 1px;"></div>
          </div> <!-- end row 3 -->




          <div class="row-fluid form-box">


            <div class="col-sm-6">
              <div class="form-horizontal">

                <div class="form-group" tal:define="field form['approvalStatus']">
                  <div class="col-sm-3">
                    <label for="approvalStatus"
                           class="col-sm-3">${structure:field.title}<span class="req" tal:condition="field.required">*</span>
                    </label>
                  </div>
                  <div class="col-sm-9">
                    ${structure:field.serialize()}
                    <ul tal:condition="field.error">
                        <li tal:repeat="error field.error.messages()">
                            ${structure:error}
                        </li>
                    </ul>
                  </div>
                </div>


                <div class="form-group" tal:condition="data.approvalTimestamp">
                  <label for="approvalTimestamp"
                  class="col-sm-3 control-label">approvalTimestamp</label>
                  <div class="col-sm-9">
                    <span tal:replace="python:data.approvalTimestamp.strftime('%Y-%m-%d %-I:%M %p')"/>
                  </div>
                </div>


                <div class="form-group" tal:condition="data.approvedBy">
                  <label for="approvedBy"
                  class="col-sm-3 control-label">approvedBy</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="approvedBy"
                    tal:attributes="value data.approvedBy" readonly>
                  </div>
                </div>


                <div class="form-group">
                  <label for="UserID"
                  class="col-sm-3 control-label">Assigned UserID</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" id="UserID"
                    tal:attributes="value data.UserID">
                  </div>
                </div>


              </div>
            </div>




            <div style="clear: both; height: 1px;"></div>
          </div> <!-- end row 4 -->

          <!--?Approval-->
          <!--?Approver-->
          <!--?ApprovalDate-->
          <!--?AdminNotes-->

          <div class="row-fluid form-box">
            <div class="col-sm-offset-2 col-sm-10">
              <button id="deformsubmit" name="submit" type="submit"
                      class="btn btn-primary " value="submit">
                Submit Update
              </button>
            </div>
            <div style="clear: both; height: 1px;"></div>
          </div> <!-- end row 4 -->


        </form>
        </div><!--!container -->
      </div>
    </div>
  </body>
</html>


